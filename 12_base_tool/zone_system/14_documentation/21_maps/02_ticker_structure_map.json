{
  "_comment": "Epoch Trading System - Module 02: Ticker Structure Cell Map",
  "_version": "1.0",
  "_organization": "XIII Trading LLC",
  "_module": "02_ticker_structure",
  "_description": "Cell mappings for dynamic ticker structure analysis (up to 10 tickers)",
  "_directory": "02_zone_system/02_ticker_structure/",

  "worksheet": "market_overview",

  "ticker_structure": {
    "_description": "User-defined tickers with market structure analysis",
    "_rows": "36-45",
    "_input": "User enters tickers in column C",
    "columns": {
      "ticker_id": "B",
      "ticker": "C",
      "datetime": "D",
      "scan_price": "E",
      "d1_dir": "F",
      "d1_strong": "G",
      "d1_weak": "H",
      "h4_dir": "I",
      "h4_strong": "J",
      "h4_weak": "K",
      "h1_dir": "L",
      "h1_strong": "M",
      "h1_weak": "N",
      "m15_dir": "O",
      "m15_strong": "P",
      "m15_weak": "Q",
      "composite": "R"
    },
    "rows": {
      "t1": 36,
      "t2": 37,
      "t3": 38,
      "t4": 39,
      "t5": 40,
      "t6": 41,
      "t7": 42,
      "t8": 43,
      "t9": 44,
      "t10": 45
    },
    "direction_values": ["Bull", "Bull+", "Bear", "Bear+", "Neutral", "ERROR"]
  },

  "ticker_validation": {
    "_description": "Rules for valid ticker symbols",
    "min_length": 1,
    "max_length": 5,
    "allowed_chars": "A-Z",
    "whitespace": "trimmed",
    "case": "uppercase"
  },

  "timeframe_weights": {
    "_description": "Weights used for composite direction calculation (same as Module 01)",
    "D1": 1.5,
    "H4": 1.5,
    "H1": 1.0,
    "M15": 0.5
  },

  "cell_reference_examples": {
    "t1_ticker": "C36",
    "t1_composite": "R36",
    "t5_d1_dir": "F40",
    "t10_h4_strong": "J45"
  }
}
