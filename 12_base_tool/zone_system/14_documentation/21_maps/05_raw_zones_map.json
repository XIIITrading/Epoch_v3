{
  "_comment": "Epoch Trading System - Module 05: Raw Zones Cell Map",
  "_version": "1.0",
  "_organization": "XIII Trading LLC",
  "_module": "05_raw_zones",
  "_description": "Cell mappings for confluence zone calculation and ranking",
  "_directory": "02_zone_system/05_raw_zones/",

  "output_worksheet": "raw_zones",

  "raw_zones_output": {
    "_description": "All zones (L1-L5) output from confluence analysis",
    "_rows": "2-N (dynamic based on zone count, up to 100 zones)",
    "header_row": 1,
    "data_start_row": 2,
    "columns": {
      "ticker_id": "A",
      "ticker": "B",
      "date": "C",
      "price": "D",
      "direction": "E",
      "zone_id": "F",
      "hvn_poc": "G",
      "zone_high": "H",
      "zone_low": "I",
      "overlaps": "J",
      "score": "K",
      "rank": "L",
      "confluences": "M"
    },
    "column_types": {
      "ticker_id": "string",
      "ticker": "string",
      "date": "string",
      "price": "float",
      "direction": "string",
      "zone_id": "string",
      "hvn_poc": "float",
      "zone_high": "float",
      "zone_low": "float",
      "overlaps": "integer",
      "score": "float",
      "rank": "string",
      "confluences": "string"
    }
  },

  "input_sources": {
    "bar_data": {
      "_description": "Confluence levels read from bar_data worksheet",
      "ticker_structure": {
        "rows": "4-13",
        "fields": ["ticker_id", "ticker", "date", "price"]
      },
      "monthly_metrics": {
        "rows": "17-26",
        "fields": ["m1_01", "m1_02", "m1_03", "m1_04", "m1_po", "m1_ph", "m1_pl", "m1_pc"]
      },
      "weekly_metrics": {
        "rows": "31-40",
        "fields": ["w1_01", "w1_02", "w1_03", "w1_04", "w1_po", "w1_ph", "w1_pl", "w1_pc"]
      },
      "daily_metrics": {
        "rows": "45-54",
        "fields": ["d1_01", "d1_02", "d1_03", "d1_04", "d1_po", "d1_ph", "d1_pl", "d1_pc"]
      },
      "time_hvn": {
        "rows": "59-68",
        "fields": ["hvn_poc1", "hvn_poc2", "hvn_poc3", "hvn_poc4", "hvn_poc5", "hvn_poc6", "hvn_poc7", "hvn_poc8", "hvn_poc9", "hvn_poc10"]
      },
      "on_options_metrics": {
        "rows": "73-82",
        "fields": ["d1_onh", "d1_onl", "op_01-10", "m5_atr", "m15_atr", "h1_atr", "d1_atr"]
      },
      "add_metrics": {
        "rows": "86-95",
        "fields": ["d1_s6", "d1_s4", "d1_s3", "d1_r3", "d1_r4", "d1_r6", "w1_s6", "w1_s4", "w1_s3", "w1_r3", "w1_r4", "w1_r6", "m1_s6", "m1_s4", "m1_s3", "m1_r3", "m1_r4", "m1_r6"]
      }
    },
    "market_overview": {
      "_description": "Market structure levels and direction from market_overview",
      "ticker_structure": {
        "rows": "36-45",
        "fields": ["direction", "d1_s", "d1_w", "h4_s", "h4_w", "h1_s", "h1_w", "m15_s", "m15_w"]
      }
    }
  },

  "scoring_configuration": {
    "poc_base_weights": {
      "hvn_poc1": 3.0,
      "hvn_poc2": 2.5,
      "hvn_poc3": 2.0,
      "hvn_poc4": 1.5,
      "hvn_poc5": 1.0,
      "hvn_poc6": 0.8,
      "hvn_poc7": 0.6,
      "hvn_poc8": 0.4,
      "hvn_poc9": 0.2,
      "hvn_poc10": 0.1
    },
    "bucket_weights": {
      "monthly_level": 3.0,
      "weekly_level": 2.0,
      "daily_level": 1.0,
      "monthly_cam": 3.0,
      "weekly_cam": 2.0,
      "daily_cam": 1.0,
      "prior_monthly": 3.0,
      "prior_weekly": 2.0,
      "prior_daily": 1.0,
      "options_level": 2.5,
      "market_structure_daily": 1.5,
      "market_structure_h4": 1.25,
      "market_structure_hourly": 1.0,
      "market_structure_m15": 0.75
    },
    "rank_thresholds": {
      "L5": {"min_score": 12.0, "description": "BEST"},
      "L4": {"min_score": 9.0},
      "L3": {"min_score": 6.0},
      "L2": {"min_score": 3.0},
      "L1": {"min_score": 0.0, "description": "WORST"}
    }
  },

  "zone_calculation": {
    "zone_width_formula": "m15_atr",
    "zone_high": "hvn_poc + (m15_atr / 2)",
    "zone_low": "hvn_poc - (m15_atr / 2)"
  },

  "cell_reference_examples": {
    "first_zone_ticker_id": "A2",
    "first_zone_rank": "L2",
    "first_zone_confluences": "M2"
  }
}
